							{
								"contentTypeId": "elementRichText",
								"contentfulId": "1PRS856SrEgcnliX8WGJtU",
								"title": "What is ISO 27001?",
								"htmlId": null,
								"elementPosition": "relative",
								"column": {
									"nodeType": "document",
									"content": [
										{
											"nodeType": "paragraph",
											"data": {
												"target": null,
