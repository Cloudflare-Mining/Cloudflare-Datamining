							{
								"contentTypeId": "elementRichText",
								"contentfulId": "18xjRvKlLkascHXARepWEJ",
								"title": "Requests for other types of information",
								"column": {
									"nodeType": "document",
									"content": [
										{
											"nodeType": "paragraph",
											"data": {
												"target": null,
