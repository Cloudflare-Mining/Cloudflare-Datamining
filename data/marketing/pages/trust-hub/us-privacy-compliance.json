							{
								"contentTypeId": "elementRichText",
								"contentfulId": "C7v0xJytY5b9BdNnPsbND",
								"title": "Is Cloudflare a \"service provider\" under CCPA/CPRA?",
								"htmlId": null,
								"elementPosition": null,
								"column": {
									"nodeType": "document",
									"content": [
