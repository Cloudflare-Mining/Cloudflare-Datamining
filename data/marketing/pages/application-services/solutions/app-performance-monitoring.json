								"contentfulId": "2RQqkJ39B3IXSfHinALyZ9",
								"title": "What does application monitoring include?",
								"htmlId": null,
								"elementPosition": null,
								"column": {
									"nodeType": "document",
									"content": [
										{
											"nodeType": "paragraph",
											"data": {
												"target": null,
												"uri": null
											},
											"value": null,
											"marks": null,
											"content": [
												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": "Application performance monitoring includes testing and real user monitoring (RUM). Metrics measured by these processes include First Input Delay (FID), Interaction to Next Paint (INP), Time to First Byte (TTFB), First Contentful Paint (FCP), Largest Contentful Paint (LCP), and Cumulative Layout Shift (CLS).",
													"marks": [],
													"content": null
												}
											]
										},
										{
											"nodeType": "paragraph",
											"data": {
												"target": null,
												"uri": null
											},
											"value": null,
											"marks": null,
											"content": [
												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": "Application performance monitoring also provides network analytics, hardware utilization metrics (e.g. disk usage), error rates, number of instances, uptime, and number of user requests.",
													"marks": [],
													"content": null
												}
											]
										}
									]
								}
							},
