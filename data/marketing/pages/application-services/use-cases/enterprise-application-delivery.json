								"contentfulId": "4q2q7ZIXeIwxDH2h5fgjGx",
								"title": "How do enterprises benefit from application delivery?",
								"htmlId": null,
								"elementPosition": "relative",
								"column": {
									"nodeType": "document",
									"content": [
										{
											"nodeType": "paragraph",
