												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": "theNET provides thought-provoking ideas, key insights, and cautionary tales to help leaders navigate crucial technology decisions and achieve their organizations' priorities with (and sometimes in spite of) today's near infinite range of technology choices and strategies.",
													"marks": [],
													"content": null
												}
											]
										},
