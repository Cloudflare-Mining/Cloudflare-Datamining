											"content": [
												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": "As a publication of Cloudflare, theNET unapologetically reflects Cloudflare's philosophies on how to build, price, and gain value from technology investments. ",
													"marks": [],
													"content": null
												}
											]
