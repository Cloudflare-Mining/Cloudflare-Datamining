												{
													"nodeType": "text",
													"data": {
														"target": null,
														"uri": null
													},
													"value": "Traditional object storage charges developers for three things: bandwidth, storage size and storage operations.",
													"marks": [],
													"content": null
												}
											]
										},
										{
											"nodeType": "paragraph",
