					"content": {
						"$ref": "assets://template/files/migrations/0001_create_user_table.sql"
					}
				},
				{
					"filename": "src/index.ts",
					"content": {
						"$ref": "assets://template/files/src/index.ts"
					}
				}
			]
		}
	],
