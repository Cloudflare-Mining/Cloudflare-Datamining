	"name": "React Router + Hono Fullstack App",
	"permissions": null,
	"setup_steps": [],
	"templates": [
		{
			"context": "create-git-connected-worker",
			"label": "React Router + Hono Fullstack App",
			"products": [
				"Workers"
			],
			"categories": [
				"starter"
			],
			"icon_urls": [],
