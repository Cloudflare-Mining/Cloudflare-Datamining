{
	"alias": "d1-template",
	"assets": [
		{
			"type": "worker",
			"name": "template",
			"build": "./",
			"files": [
				".gitignore",
				"README.md",
				"package.json",
				"tsconfig.json",
				"worker-configuration.d.ts",
				"wrangler.json",
				"migrations/0001_create_comments_table.sql",
				"src/index.ts",
				"src/renderHtml.ts"
			],
			"replacements": {
				"d1-template": {
					"$ref": "values://target/worker/name"
				},
				"d1-template-database": {
					"$ref": "values://resources/DBD1Database/result/name"
				},
				"151f7d9b-365f-41d7-83ed-0bf4eeef5086": {
					"$ref": "values://resources/DBD1Database/result/id"
				}
			}
		}
	],
	"category": "templates",
	"description": "Cloudflare's native serverless SQL database.",
	"docs_url": "https://developers.cloudflare.com/d1/",
	"flags": [
		"template_only"
	],
	"get_data_action": null,
	"icon_url": null,
	"install_target": "worker",
	"name": "Worker + D1 Database",
	"permissions": null,
	"setup_steps": [
		{
			"title": "Create D1 Database",
			"type": "form",
			"components": [
				{
					"type": "text_input",
					"name": "DBD1DatabaseName",
					"label": "Name your D1 Database",
					"required": true,
					"default_value": "d1-template-database"
				},
				{
					"type": "select_input",
					"name": "DBD1DatabaseLocationHint",
					"label": "Database location hint",
					"required": true,
					"default_value": "automatic",
					"options": {
						"type": "static",
						"static": [
							{
								"label": "Automatic: D1 auto-assigns your database to the nearest available region.",
								"value": "automatic"
							},
							{
								"label": "Western North America",
								"value": "WNAM"
							},
							{
								"label": "Eastern North America",
								"value": "ENAM"
							},
							{
								"label": "Western Europe",
								"value": "WEUR"
							},
							{
								"label": "Eastern Europe",
								"value": "EEUR"
							},
							{
								"label": "Asia-Pacific",
								"value": "APAC"
							}
						]
					}
				}
			],
			"resources": [
				{
					"name": "DBD1Database",
					"type": "d1",
					"params": {
						"name": {
							"$ref": "values://data/DBD1DatabaseName"
						},
						"locationHint": {
							"$ref": "values://data/DBD1DatabaseLocationHint"
						}
					}
				}
			]
		}
	],
	"templates": [
		{
			"context": "create-git-connected-worker",
			"label": "Worker + D1 Database",
			"products": [
				"Workers",
				"D1"
			],
			"categories": [
				"storage"
			],
			"icon_urls": [
