											],
											"type": "object"
										},
										"serie_0": {
											"additionalProperties": {
												"items": {
													"anyOf": [
														{
															"type": "string"
														},
														{
															"type": "number"
														}
													]
												},
												"type": "array"
											},
											"example": {
												"google.com": [
													2
												],
												"timestamps": [
													"2022-09-02"
												]
											},
											"properties": {
												"timestamps": {
													"items": {
														"type": "string"
													},
													"type": "array"
												}
											},
											"required": [
												"timestamps"
											],
											"type": "object"
										}
									},
									"required": [
										"meta",
										"serie_0"
									],
									"type": "object"
								},
								"success": {
									"example": true,
									"type": "boolean"
								}
							},
							"required": [
