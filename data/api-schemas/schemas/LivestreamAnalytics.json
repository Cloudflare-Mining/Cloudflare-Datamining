{
	"GET /accounts/{account_id}/realtime/kit/{app_id}/analytics/livestreams/daywise": {
		"description": "Returns day-wise livestream analytics data for the specified time range.",
		"operationId": "get-livestream-analytics-daywise",
		"parameters": [
			{
				"in": "path",
				"name": "account_id",
				"required": true,
				"schema": {
					"description": "The account identifier tag.",
					"example": "023e105f4ecef8ad9ca31a8372d0c353",
					"maxLength": 32,
					"type": "string",
					"x-auditable": true
				}
			},
			{
				"in": "path",
				"name": "app_id",
				"required": true,
				"schema": {
					"description": "The app identifier tag.",
					"example": "14a396e7-ca44-4937-bf1f-050a69118543",
					"maxLength": 32,
					"type": "string"
				}
			},
			{
				"description": "Specify the start time range in ISO format to access the livestream analytics.",
				"in": "query",
				"name": "start_time",
				"schema": {
					"format": "date-time",
					"type": "string"
				}
			},
			{
				"description": "Specify the end time range in ISO format to access the livestream analytics.",
				"in": "query",
				"name": "end_time",
				"schema": {
					"format": "date-time",
					"type": "string"
				}
			}
		],
		"responses": {
			"200": {
				"content": {
					"application/json": {
						"examples": {
							"Example 1": {
								"value": {
									"data": [
										{
											"count": 4,
											"date": "2023-07-15T00:00:00.000Z",
											"total_ingest_seconds": 531,
											"total_viewer_seconds": 116
										}
									],
									"success": true
								}
							}
						},
						"schema": {
							"properties": {
								"data": {
									"properties": {
										"count": {
											"description": "Count of total livestreams.",
											"type": "string"
										},
										"date": {
											"description": "The livestream timestamp, provided in ISO format.",
											"type": "string"
										},
										"total_ingest_seconds": {
											"description": "Total time duration for which the input was given or the meeting was streamed.",
											"type": "string"
										},
										"total_viewer_seconds": {
											"description": "Total view time for which the viewers watched the stream.",
											"type": "string"
										}
									},
									"type": "object"
								},
								"success": {
									"type": "boolean"
								}
							},
							"type": "object"
						}
					}
				},
				"description": "OK"
			}
		},
		"security": [
			{
				"api_token": []
			}
		],
		"summary": "Fetch day-wise analytics data for your livestreams",
		"tags": [
			"Live streams",
			"LivestreamAnalytics"
		],
		"x-api-token-group": [
			"Realtime Admin",
			"Realtime"
		]
	},
