			}
		},
		"security": [
			{
				"api_token": []
			},
			{
				"api_email": [],
				"api_key": []
			}
		],
		"summary": "Execute @cf/myshell-ai/melotts model.",
		"tags": [
			"Workers AI Text To Speech"
		],
		"x-api-token-group": [
			"Workers AI Write",
			"Workers AI Read"
		],
		"x-cfPermissionsRequired": {
			"enum": [
				"com.cloudflare.api.account.ai"
			]
		},
		"x-cfPlanAvailability": {
			"business": true,
			"enterprise": true,
			"free": true,
			"pro": true
		}
	}
