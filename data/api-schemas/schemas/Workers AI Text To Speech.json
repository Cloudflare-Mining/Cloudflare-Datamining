{
	"POST /accounts/{account_id}/ai/run/@cf/myshell-ai/melotts": {
		"operationId": "workers-ai-post-run-cf-myshell-ai-melotts",
		"parameters": [
			{
				"in": "path",
				"name": "account_id",
				"required": true,
				"schema": {
					"example": "023e105f4ecef8ad9ca31a8372d0c353",
					"type": "string"
				}
			},
			{
				"in": "query",
				"name": "queueRequest",
