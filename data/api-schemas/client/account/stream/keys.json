	"type": "object",
	"links": [
		{
			"title": "Create signing keys",
			"description": "Creates an RSA private key in PEM and JWK formats. Key files are only displayed once after creation. Keys are created, used, and deleted independently of videos, and every key can sign any video.",
			"rel": "collection",
			"href": "accounts/{#/definitions/account_identifier}/stream/keys",
			"method": "POST",
			"cfOwnership": "stream",
			"html_id": "stream-signing-keys-create-signing-keys",
			"uri": "accounts/:account_identifier/stream/keys",
			"curl": "curl -X POST \"https://api.cloudflare.com/client/v4/accounts/9a7806061c88ada191ed06f989cc3dac/stream/keys\" \\\n     -H \"X-Auth-Email: user@example.com\" \\\n     -H \"X-Auth-Key: c2547eb745079dac9320b638f5e225cf483cc5cfdda41\" \\\n     -H \"Content-Type: application/json\"",
			"response": "{\n  \"success\": true,\n  \"errors\": [],\n  \"messages\": [],\n  \"result\": {\n    \"id\": \"5213cfa121f70b8c1380686ffc371ba3\",\n    \"pem\": \"\",\n    \"jwk\": \"\",\n    \"created\": \"2014-01-02T02:20:00Z\"\n  }\n}"
		},
		{
			"title": "List signing keys",
			"description": "Lists the video ID and creation date and time when a signing key was created.",
			"rel": "collection",
			"href": "accounts/{#/definitions/account_identifier}/stream/keys",
			"method": "GET",
			"cfOwnership": "stream",
			"html_id": "stream-signing-keys-list-signing-keys",
			"uri": "accounts/:account_identifier/stream/keys",
			"curl": "curl -X GET \"https://api.cloudflare.com/client/v4/accounts/9a7806061c88ada191ed06f989cc3dac/stream/keys\" \\\n     -H \"X-Auth-Email: user@example.com\" \\\n     -H \"X-Auth-Key: c2547eb745079dac9320b638f5e225cf483cc5cfdda41\" \\\n     -H \"Content-Type: application/json\"",
			"response": "{\n  \"success\": true,\n  \"errors\": [],\n  \"messages\": [],\n  \"result\": [\n    {\n      \"id\": \"5213cfa121f70b8c1380686ffc371ba3\",\n      \"created\": \"2014-01-02T02:20:00Z\"\n    }\n  ]\n}"
		},
		{
			"title": "Delete signing keys",
			"description": "Deletes signing keys and revokes all signed URLs generated with the key.",
