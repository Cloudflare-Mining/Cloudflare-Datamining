			"response": "{\n  \"success\": true,\n  \"errors\": [],\n  \"messages\": [],\n  \"result\": {\n    \"variant\": {\n      \"id\": \"hero\",\n      \"options\": {\n        \"fit\": \"scale-down\",\n        \"metadata\": \"none\",\n        \"width\": 1366,\n        \"height\": 768\n      },\n      \"neverRequireSignedURLs\": true\n    }\n  }\n}"
