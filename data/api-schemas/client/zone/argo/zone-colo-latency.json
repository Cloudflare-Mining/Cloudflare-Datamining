					"description": "Latency analytics for one PoP",
					"type": "object",
					"properties": {
						"properties": {
							"description": "Information of Argo latency at the PoP",
							"type": "object",
							"properties": {
								"code": {
									"description": "Code of PoP. The airport code",
									"type": "string",
									"example": "ATL"
								},
								"argo_req_count": {
									"description": "Number of requests Argo routed",
									"type": "number",
									"example": 15644
								},
								"pct_avg_change": {
									"description": "Percentage of improvement of average latency by Argo. This is calculated as (Argo - NoArgo)/NoArgo.",
									"type": "number",
									"example": -0.38009502863254624
								},
								"no_argo_avg": {
									"description": "Average latency of requests not using Argo, in milliseconds",
									"type": "number",
									"example": 319.0564263322884
								},
								"argo_avg": {
									"description": "Average latency of requests using Argo, in milliseconds",
									"type": "number",
									"example": 197.78466483011937
								}
							}
						},
						"type": {
							"description": "Feature",
							"type": "string",
							"enum": [
								"Feature"
							],
							"example": "Feature"
						},
						"geometry": {
							"description": "Geolocation of the PoP",
							"type": "object",
							"properties": {
								"type": {
									"description": "Geolocation type",
									"type": "string",
									"enum": [
										"Point"
									],
									"example": "Point"
								},
								"coordinates": {
									"description": "Longitude and latitude of the PoP",
									"type": "array",
									"items": {
										"type": "number"
									},
									"minItems": 2,
									"maxItems": 2,
									"example": [
										-84.44403,
										33.640068
									]
								}
							}
						}
					}
				}
			}
