---
import FlagsTable from '../../components/FlagsTable.astro';
import Layout from '../../layouts/Layout.astro';
export const prerender = true;

const importFiles = await Astro.glob('../../../../data/account/zone_flags.json');
const flags = importFiles[0].default;

// manual overrides
if (flags.workers.cloudflare_pages_functions_daily_limit) {
	flags.workers.cloudflare_pages_functions_daily_limit = 100000;
}
---
<Layout title="Zone Flags - Cloudflare Datamining" description="Cloudflare zone feature flags">
	<FlagsTable title="Zone Flags" data={flags} headers={['Flag', 'Default Value']}></FlagsTable>
</Layout>

